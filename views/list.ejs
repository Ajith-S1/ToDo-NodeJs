 <%- include("header") -%>

  <div class="box" id="heading">
    <h1> <%= listTitle %> </h1>
  </div>

  <div class="box">
    <% for(let i=0 ;i<newListItem.length ;i++){ %>
    <div class="item">
      <input type="checkbox">
      <p><%= newListItem[i] %></p>

    </div>
    <%  } %>

    <form class="" action="/" method="post">
      <input type="text" name="news" placeholder="new-Item" autocomplete="off">
      <button type="submit" name="list" value= <%= listTitle %>>+</button><br>

    </form>
  </div>
  <%- include("footer") -%>
